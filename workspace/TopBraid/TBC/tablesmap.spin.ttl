tablesmap:rowName
  rdf:type spin:Function ;
  spin:body [
      rdf:type sp:Select ;
      sp:resultVariables (
          [
            sp:varName "value"^^xsd:string ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "type"^^xsd:string ;
              ] ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "row"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type tablesmap:maxColIndex ;
                arg:firstColIndex [
                    sp:varName "firstColIndex"^^xsd:string ;
                  ] ;
                arg:lastColIndex [
                    sp:varName "lastColIndex"^^xsd:string ;
                  ] ;
                arg:pairs [
                    sp:varName "pairs"^^xsd:string ;
                  ] ;
                sp:arg1 spin:_arg1 ;
              ] ;
            sp:variable [
                sp:varName "colIndex"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:bound ;
                sp:arg1 [
                    sp:varName "colIndex"^^xsd:string ;
                  ] ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type tables:cellByColIndex ;
                arg:colIndex [
                    sp:varName "colIndex"^^xsd:string ;
                  ] ;
                arg:row spin:_arg1 ;
              ] ;
            sp:variable [
                sp:varName "value"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:optional "true"^^xsd:boolean ;
      spl:predicate arg:pairs ;
      spl:valueType xsd:boolean ;
      rdfs:comment "true to group columns into pairs, with the left one holding the ID."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:firstColIndex ;
      spl:valueType xsd:integer ;
      rdfs:comment "The start column index."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:lastColIndex ;
      spl:valueType xsd:integer ;
      rdfs:comment "The end column index."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate sp:arg1 ;
      spl:valueType rdfs:Resource ;
      rdfs:comment "The row instance."^^xsd:string ;
    ] ;
  spin:private "true"^^xsd:boolean ;
  spin:returnType xsd:string ;
  rdfs:comment "Gets the content of the last column of a given row, between a given first and last columns."^^xsd:string ;
  rdfs:label "row name"^^xsd:string ;
  rdfs:subClassOf tablesmap:TableFunctions ;
.
tablesmap:rowWithName
  rdf:type spin:Function ;
  spin:body [
      rdf:type sp:Select ;
      sp:resultVariables (
          [
            sp:varName "row"^^xsd:string ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "firstColIndex"^^xsd:string ;
              ] ;
            sp:predicate tables:columnIndex ;
            sp:subject [
                sp:varName "firstColPredicate"^^xsd:string ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "lastColIndex"^^xsd:string ;
              ] ;
            sp:predicate tables:columnIndex ;
            sp:subject [
                sp:varName "lastColPredicate"^^xsd:string ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "name"^^xsd:string ;
              ] ;
            sp:predicate [
                sp:varName "p"^^xsd:string ;
              ] ;
            sp:subject [
                sp:varName "row"^^xsd:string ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "colIndex"^^xsd:string ;
              ] ;
            sp:predicate tables:columnIndex ;
            sp:subject [
                sp:varName "p"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:and ;
                sp:arg1 [
                    rdf:type sp:ge ;
                    sp:arg1 [
                        sp:varName "colIndex"^^xsd:string ;
                      ] ;
                    sp:arg2 [
                        sp:varName "firstColIndex"^^xsd:string ;
                      ] ;
                  ] ;
                sp:arg2 [
                    rdf:type sp:le ;
                    sp:arg1 [
                        sp:varName "colIndex"^^xsd:string ;
                      ] ;
                    sp:arg2 [
                        sp:varName "lastColIndex"^^xsd:string ;
                      ] ;
                  ] ;
              ] ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:or ;
                sp:arg1 [
                    rdf:type sp:ne ;
                    sp:arg1 [
                        sp:varName "pairs"^^xsd:string ;
                      ] ;
                    sp:arg2 "true"^^xsd:boolean ;
                  ] ;
                sp:arg2 [
                    rdf:type sp:eq ;
                    sp:arg1 [
                        rdf:type spif:mod ;
                        sp:arg1 [
                            sp:varName "firstColIndex"^^xsd:string ;
                          ] ;
                        sp:arg2 2 ;
                      ] ;
                    sp:arg2 [
                        rdf:type spif:mod ;
                        sp:arg1 [
                            sp:varName "colIndex"^^xsd:string ;
                          ] ;
                        sp:arg2 2 ;
                      ] ;
                  ] ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "type"^^xsd:string ;
              ] ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "row"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type tables:cellByColIndex ;
                arg:colIndex [
                    rdf:type sp:add ;
                    sp:arg1 [
                        sp:varName "colIndex"^^xsd:string ;
                      ] ;
                    sp:arg2 [
                        rdf:type sp:if ;
                        sp:arg1 [
                            sp:varName "pairs"^^xsd:string ;
                          ] ;
                        sp:arg2 2 ;
                        sp:arg3 1 ;
                      ] ;
                  ] ;
                arg:row [
                    sp:varName "row"^^xsd:string ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "rightName"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:or ;
                sp:arg1 [
                    rdf:type sp:not ;
                    sp:arg1 [
                        rdf:type sp:bound ;
                        sp:arg1 [
                            sp:varName "rightName"^^xsd:string ;
                          ] ;
                      ] ;
                  ] ;
                sp:arg2 [
                    rdf:type sp:eq ;
                    sp:arg1 [
                        sp:varName "colIndex"^^xsd:string ;
                      ] ;
                    sp:arg2 [
                        sp:varName "lastColIndex"^^xsd:string ;
                      ] ;
                  ] ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:firstColPredicate ;
      spl:valueType rdf:Property ;
      rdfs:comment "The first column property of the tree."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:lastColPredicate ;
      spl:valueType xsd:integer ;
      rdfs:comment "The end column property of the tree."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:name ;
      rdfs:comment "The name to match."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:pairs ;
      spl:valueType xsd:boolean ;
      rdfs:comment "true to group columns into pairs, with the left one holding the ID."^^xsd:string ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:type ;
      spl:valueType rdfs:Class ;
      rdfs:comment "The class to consider instances of."^^xsd:string ;
    ] ;
  spin:private "true"^^xsd:boolean ;
  rdfs:comment "Gets the row instance of a given class (?type) that has a given name (?name) within start and end column properties. This is defined as the instance that has that value as its last column (i.e. the right neighbor cell must be empty or the end column."^^xsd:string ;
  rdfs:label "row with name"^^xsd:string ;
  rdfs:subClassOf tablesmap:TableFunctions ;
.
